{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Okhaz": {
      "root": "e2e",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "@firebase/app",
              "firebase/app",
              "withinviewport",
              "chart.js",
              "lodash",
              "babel-runtime",
              "nvd3",
              "rxjs-compat",
              "angular2-text-mask",
              "leaflet-map",
              "dragula",
              "ng2-validation",
              "leaflet",
              "@mattlewis92/dom-autoscroller",
              "contra/emitter",
              "crossvent",
              "dom-plane",
              "dom-set",
              "babel-runtime/core-js/get-iterator",
              "babel-runtime/helpers/extends",
              "babel-runtime/helpers/slicedToArray",
              "babel-runtime/helpers/createClass",
              "lodash/throttle",
              "hogan.js",
              "lodash/uniq",
              "lodash/curry",
              "lodash/escape",
              "lodash/union",
              "lodash/isFinite",
              "lodash/isEqual",
              "lodash/filter",
              "angular-calendar/date-adapters/date-fns",
              "ngx-embed-video",
              "calendar-utils/date-adapters/date-fns",
              "core-js/library/fn/get-iterator",
              "angular-instantsearch",
              "nouislider",
              "algoliasearch-helper",
              "algoliasearch/lite",
              "algoliasearch/index",
              "zrender",
              "echarts"
            ],
            "sourceMap": false,
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": ["src/assets"],
            "styles": [
              "src/assets/scss/style.scss",
              "node_modules/@videogular/ngx-videogular/fonts/videogular.css",
              "node_modules/angular-instantsearch/bundles/instantsearch.min.css",
              "node_modules/angular-instantsearch/bundles/instantsearch-theme-algolia.min.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/angular-calendar/css/angular-calendar.css",
              "node_modules/@swimlane/ngx-datatable/index.css",
              "node_modules/@swimlane/ngx-datatable/themes/material.css",
              "node_modules/@swimlane/ngx-datatable/assets/icons.css",
              "node_modules/nvd3/build/nv.d3.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/cryptocoins-icons/webfont/cryptocoins.css",
              "src/assets/fonts/data-table/icons.css",
              "node_modules/dragula/dist/dragula.css",
              "node_modules/@circlon/angular-tree-component/css/angular-tree-component.css",
              "node_modules/nouislider/distribute/nouislider.min.css",
              "node_modules/slick-carousel/slick/slick.scss",
              "node_modules/slick-carousel/slick/slick-theme.scss",
              "node_modules/ngx-bar-rating/themes/br-default-theme.scss"
            ],
            "scripts": [
              "src/assets/js/Winwheel.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/slick-carousel/slick/slick.min.js",
              "node_modules/chart.js/dist/Chart.bundle.min.js",
              "node_modules/easy-pie-chart/dist/easypiechart.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "Okhaz:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "Okhaz:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Okhaz:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/slick-carousel/slick/slick.min.js",
              "node_modules/chart.js/dist/Chart.bundle.min.js",
              "node_modules/easy-pie-chart/dist/easypiechart.js"
            ],
            "styles": [
              "src/assets/scss/style.scss",
              "node_modules/@videogular/ngx-videogular/fonts/videogular.css",
              "node_modules/angular-instantsearch/bundles/instantsearch.min.css",
              "node_modules/angular-instantsearch/bundles/instantsearch-theme-algolia.min.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/angular-calendar/css/angular-calendar.css",
              "node_modules/@swimlane/ngx-datatable/index.css",
              "node_modules/@swimlane/ngx-datatable/themes/material.css",
              "node_modules/@swimlane/ngx-datatable/assets/icons.css",
              "node_modules/nvd3/build/nv.d3.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/cryptocoins-icons/webfont/cryptocoins.css",
              "src/assets/fonts/data-table/icons.css",
              "node_modules/dragula/dist/dragula.css",
              "node_modules/@circlon/angular-tree-component/css/angular-tree-component.css",
              "node_modules/nouislider/distribute/nouislider.min.css",
              "node_modules/slick-carousel/slick/slick.scss",
              "node_modules/slick-carousel/slick/slick-theme.scss",
              "node_modules/ngx-bar-rating/themes/br-default-theme.scss"
            ],
            "assets": ["src/assets"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["tsconfig.app.json", "tsconfig.spec.json"],
            "exclude": []
          }
        }
      }
    },
    "Okhaz-e2e": {
      "root": "",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "Okhaz:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["e2e/tsconfig.e2e.json"],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "Okhaz",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "ms",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "ms"
    }
  },
  "cli": {
    "analytics": "ee0747fb-a5e5-4055-b490-2a34df39a7b5"
  }
}
